{
    "log": {
        "console": {
            "level": "debug",
            "outputTimestamp": false,
            "outputLevel": true
        },
        "file": {
            "level": "debug",
            "directory": "logs"
        },
        "eventLog": {
            "level": "none"
        },
        "appendLocation": false
    },
    "server": {
        "ipv4Only": false,
        "port": 5757,
        "concurrencyLimit": 10,
        "enableLegacyTls": "no",
        "securityLevel": 5,
        "certificateChainFile": "conf\\kkmha.test.ss.crt",
        "privateKeyFile": "conf\\kkmha.test.ss.key",
        "privateKeyPassword": "",
        "secret": "lorem.ipsum.dolor.sit.amet",
        "loopbackWithoutSecret": false,
        "enableStatic": false,
        "enableUnknownType": false,
        "staticDirectory": "static",
        "mimeMap": "conf\\mime.json"
    },
    "kkm": {
        "dbDirectory": "conf\\kkm",
        "defaultBaudRate": 115200,
        "defaultLineLength": 42,
        "timeZone": "device",
        "documentClosingTimeout": 5000,
        "cliOperator": {
            "name": "Оператор",
            "--inn": "770000000000"
        },
        "maxCashInOut": 500000,
        "maxPrice": 300000,
        "maxQuantity": 1000
    }
}